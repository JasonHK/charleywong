{
  "name": "Charley Wong 和你查 link check",
  "version": "0.1",
  "description": "Display a button next to links that Charley Wong knows about.",
  "manifest_version": 2,
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "exclude_matches": ["https://charleywong.giffon.io/*"],
      "js": ["Content.js"],
      "css": ["style.css"]
    }
  ]
}
